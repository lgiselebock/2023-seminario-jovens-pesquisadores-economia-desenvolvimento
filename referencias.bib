@article{bortoluzzi_aspectos_2015,
	title = {Aspectos do {Endividamento} das {Famílias} {Brasileiras} no {Período} de 2011-2014},
	volume = {39},
	url = {https://www.uricer.edu.br/site/pdfs/perspectiva/146_513.pdf},
	abstract = {Credit is an economic tool that enables advance purchasing power, and thus generate the expansiveness of the market activities. Greater access to credit may bring benefits to the consumer, such as better quality of life, but at some level the debtor may not be able to afford his/her obligations, turning into an indebted. In this terms, this article aims to analyze the level of debt of Brazilian families in the period from 2011 to 2014. It was used a descriptive research based on secondary data obtained from the Brazilian Central Bank websites, the Credit Protection Service and the National Debt and Consumer Default Search. The results show a high percentage of indebtedness and the occurrence of increasing and decreasing fluctuations during the study period. The most used credit forms by Brazilians between 2011 and 2014 were personal credit and real estate financing. It is concluded that the different credit options available today led the country’s economy to register alarming rates in relation to household debt.},
	language = {pt},
	number = {n. 146},
	journal = {Perspectiva},
	author = {Bortoluzzi, Daiane Antonini and Boligon, Juliana Andreia Rüdell and Hollveg, Scheila Daiana Severo and Medeiros, Flaviani Souto Bolzan},
	year = {2015},
	pages = {111--123},
	file = {Bortoluzzi et al. - ASPECTOS DO ENDIVIDAMENTO DAS FAMÍLIAS BRASILEIRAS.pdf:/Users/luisagisele/Zotero/storage/HK2JXHU2/Bortoluzzi et al. - ASPECTOS DO ENDIVIDAMENTO DAS FAMÍLIAS BRASILEIRAS.pdf:application/pdf},
